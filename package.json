    "ngx-highlightjs": "^4.1.4",
    "ngx3js": "file:../ngx3js-module/dist/ngx3js",
    "path": "^0.12.7",
    "rxjs": "^6.6.7",
    "tslib": "^2.3.1",
    "webpack": "^5.58.2",
