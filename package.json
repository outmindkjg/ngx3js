{
  "name": "three-sample",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "maxmem": "node --max_old_space_size=4000 ./node_modules/@angular/cli/bin/ng serve --port 4300",
    "ngx3jswatch": "cd ../ngx3js-module/ && ng build ngx3js --watch",
    "maxbuild": "node --max_old_space_size=4000 ./node_modules/@angular/cli/bin/ng build",
    "maxbuildprod": "node --max_old_space_size=4000 ./node_modules/@angular/cli/bin/ng build --configuration production",
    "doccopy": "rm -rf docs && cp -rf ../ngx3js-module/docs ./ &&  rm -rf ../ngx3js-doc/* && cp -rf dist/ngx3js-doc/* ../ngx3js-doc/ && cp -rf ../ngx3js-module/docs ../ngx3js-doc/ && cp ../ngx3js-module/README.md ../ngx3js-doc/ "
  },
  "browser": {
    "crypto": false,
    "fs": false,
    "path": false,
    "os": false,
    "net": false,
    "stream": false,
    "tls": false
  },
  "private": true,
